@{
    ViewBag.Title = "Панель администратора";
}

<h2>Панель администратора</h2>
<hr />
<div class="row content">
    <div class="col-md-3">
        <div class="panel panel-default">
            <div class="panel-heading">Данные о пользователях</div>
            <div class="panel-body">
                <div class="list-group">
                    @Html.ActionLink("Роли", "GetRole", "Admin", new { @class = "list-group-item" })
                    @Html.ActionLink("Пользователи", "GetUser", "Admin", new { @class = "list-group-item" })
                </div>
            </div>
        </div>        
    </div>
    <div class="row content">
        <div class="col-md-9">
            <a class="btn btn-default btn-lg btn-block" href="@Url.Action("CreateQuiz", "Admin")">Создать викторину</a>
        </div>
    </div>
    
        
    
</div>

